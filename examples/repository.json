{
    "repositories": {
        "maven2": {
            "proxy": {
                "maven-central": {
                    "remote": {
                        "url": "https://repo1.maven.org/maven2/"
                    },
                    "blobstore": {
                        "name": "default"
                    }
                }
            },
            "hosted": {
                "maven-releases": {
                    "blobstore": {
                        "name": "default"
                    },
                    "layout_policy": "strict"
                },
                "maven-snapshots": {
                    "blobstore": {
                        "name": "default"
                    },
                    "version_policy": "snapshot",
                    "layout_policy": "strict",
                    "write_policy": "allow"
                }
            },
            "group": {
                "maven-public": {
                    "repositories": [
                        "maven-releases",
                        "maven-snapshots",
                        "maven-central"
                    ],
                    "blobstore": {
                        "name": "default",
                        "strict_content_type_validation": "true"
                    }
                }
            }
        },
        "nuget": {
            "proxy": {
                "nuget.org-proxy": {
                    "remote": {
                        "url": "https://www.nuget.org/api/v2/",
                        "content_max_age": "1440"
                    },
                    "blobstore": {
                        "name": "default",
                        "strict_content_type_validation": "true"
                    }
                }
            },
            "hosted": {
                "nuget-hosted": {
                    "blobstore": {
                        "name": "default",
                        "strict_content_type_validation": "true"
                    },
                    "write_policy": "allow"
                }
            },
            "group": {
                "nuget-group": {
                    "repositories": [
                        "nuget-hosted",
                        "nuget.org-proxy"
                    ],
                    "blobstore": {
                        "name": "default",
                        "strict_content_type_validation": "true"
                    }
                }
            }
        },
        "apt": {
            "proxy": {
                "proxy-apt-ubuntu-bionic": {
                    "remote": {
                        "url": "http://archive.ubuntu.com/ubuntu/"
                    },
                    "blobstore": {
                        "name": "default"
                    },
                    "distribution": "bionic"
                },
                "proxy-apt-ubuntu-bionic-security": {
                    "remote": {
                        "url": "http://archive.ubuntu.com/ubuntu/"
                    },
                    "blobstore": {
                        "name": "default"
                    },
                    "distribution": "bionic-security"
                }
            }
        }
    },
    "blobstores": {
        "file": [
            "default"
        ]
    }
}
